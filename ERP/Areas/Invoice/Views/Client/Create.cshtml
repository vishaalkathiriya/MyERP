@{
    ViewBag.Title = "Create";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<div class="row">
    <div class="col-sm-12">
        <div class="page-header main-page-header">
            <h2><i class="entypo-doc-text"></i>KYC
                <span style="float: right;">
                    <a class="btn btn-sm btn-info btn-icon icon-left" href="/Invoice/Client/Create"><i class="entypo-plus"></i>Add New</a>
                    <a class="btn btn-sm btn-primary btn-icon icon-left" href="/Invoice/Client"><i class="entypo-reply"></i>Back To List</a>
                </span>
            </h2>
        </div>
    </div>
</div>

<div data-ng-controller="ClientCreateMasterCtrl">
