        <div class="col-sm-12 employee-tabs">
            <div data-tabset>
                <!-- BEGIN CLIENT CREATE PARTIALLY-->
                @Html.Partial("_ClientCreatePartially")
                <!-- / END CLIENT CREATE PARTIALLY-->

                <!-- BEGIN CLIENT CREATE FULL-->
                @Html.Partial("_ClientCreateFull")
                <!-- / END CLIENT CREATE FULL-->
            </div>
        </div>
    </div>
</div>

@section Scripts{
    @Scripts.Render("~/Area/Invoice/Scripts/invoiceClientMaster")
}